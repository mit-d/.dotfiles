" vimrc 1.0 by mit-d
set nocp
set ttyfast
set nonu
set nornu
set nolist
set mouse=a
set ru
set wmnu
set ls=2
set ar

set nowrap
set nolbr
set sbr=(+)
set tw=0
set so=5

set ai
set shiftwidth=4
set sts=4
set et
set si
set sta

set fdm=manual

set hls
set is
set sm
set scs
set ic
set magic

filetype plugin indent on
set background=dark
sy off
let g:dotvim = '~/.vim'

